class Solution {
    public int climbStairs(int n) {
         int[] st=new int[n+1];
        int ans=0;
        if(n<3)
        {
            ans=n;

        }
        
    
     st[1]=1;
     st[2]=2;
     for(int i=3;i<=n;i++)
     {
        st[i]=st[i-1]+st[i-2];
     }
        
     return st[n];
    }
}
