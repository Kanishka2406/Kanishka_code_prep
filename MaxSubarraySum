class Solution {
    public int maxSubArray(int[] nums) {
        int n=nums.length;
        int cs=0,maxsum=nums[0];
        for(int i=0;i<n;i++)
        {
            cs+=nums[i];
            maxsum=Math.max(maxsum,cs);
        if(cs<0)
        {
            cs=0;
        }}
        return maxsum;
        
    }
}
